schema: 'https://rickandmortyapi.com/graphql'
extensions:
  graphql-markdown:
    rootPath: ./docs
    baseURL: /
    homepage: static/index.md
    loaders:
      UrlLoader: '@graphql-tools/url-loader'
    docOptions:
      frontMatter:
        pagination_next: null
        pagination_prev: null
        hide_table_of_contents: true
      index: true
    printTypeOptions:
      deprecated: group
